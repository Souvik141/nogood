{
  "name": "npgood",
  "version": "0.1.0",
  "private": false,
  "description": "not have any, for now",
  "scripts": {
    "start": "nodemon back/server.js",
    "server": "nodemon back/server.js",
    "client": "npm start --prefix front",
    "pretty": "npm run-script pretty --prefix front && npm run-script pretty --prefix back"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Souvik141/front-client.git"
  },
  "author": "sav",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "jsxSingleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
  },
  "bugs": {
    "url": "https://github.com/Souvik141/front-client/issues"
  },
  "homepage": "https://github.com/Souvik141/front-client#readme",
  "main": "index.js",
  "license": "ISC"
}
